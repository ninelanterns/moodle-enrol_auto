    "type": "totara-enrol",
    "require": {
        "kineo/installers": "~1.3",
        "tke/tke": ">=13.4"
    },
    "extra": {
        "installer-name": "auto",
        "kineo": {
            "code-standards": "moodle",
            "has-config-init": false,
